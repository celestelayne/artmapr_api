<div class="col-md-8 col-md-offset-2">
  <div class="col-xs-6 col-md-4">
    <h1>Art Profile</h1>
  </div>
  <div class="">
    <p><%= image_tag @art.photo.url(:small) %></p>
    <p><strong>Title: </strong><%= @art.title %></p>
    <p><strong>Name: </strong><%= @art.artist_first_name %> <%= @art.artist_last_name %></p>
    <p><strong>Medium: </strong><%= @art.medium %></p>
  </div>
</div>
